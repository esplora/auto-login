{
  "name": "esplora/auto-login",
  "description": "Enables seamless autologin functionality via temporary links, providing convenient and secure access to your application.",
  "keywords": ["laravel", "autoauth", "autologin"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com",
      "homepage": "https://tabuna.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": "^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.2",
    "orchestra/testbench-core": "^8.0",
    "orchestra/testbench-dusk": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Esplora\\AutoLogin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Esplora\\AutoLogin\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
